<Window x:Class="EjemploBindingObjetos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EjemploBindingObjetos"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
        <StackPanel x:Name="PanelStackPanel">
        <TextBlock Text="Nombre"/>
        <TextBox x:Name="NombreTextBox" Text="{Binding Path=Nombre}"></TextBox>
        <TextBlock Text="Edad"/>
        <TextBox x:Name="EdadTextBox" Text="{Binding Path=Edad}"></TextBox>
        <Button Content="Mostrar" Click="Button_Click"></Button>
        <Button Content="Cumpleaños" Click="Button_Click_1"></Button>
    </StackPanel>
   
</Window>
